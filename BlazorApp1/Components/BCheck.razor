<div class="form-check">
  <input class="form-check-input" type="checkbox" value="" id="bCheck-@bCheckbox.Id" @bind="bCheckbox.Active">
  <label class="form-check-label" for="flexCheckDefault">
    @bCheckbox.Title
  </label>
</div>
@code
{
    [Parameter]
    public BCheckbox bCheckbox { get; set; }
}